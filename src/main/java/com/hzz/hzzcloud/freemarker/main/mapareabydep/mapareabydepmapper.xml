<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--区域围栏和机构权限表mapper -->
<mapper namespace="mapareabydepmapper">

    <!--查询区域围栏和机构权限表列表 -->
    <select id="selectlist" parameterType="java.util.HashMap"
            resultType="java.util.HashMap">
        select
                <!--  主键   -->
                a.id,
                <!--  配置id   -->
                a.configid,
                <!--  绑定机构的id   -->
                a.depid,
                <!--  创建时间   -->
                a.createdate,
                <!--  删除标志,0代表未删除,1代表删除   -->
                a.deleted
        from
        subiaodb.mapareabydep as a

        where 1=1
                <if test="id != null ">
                    and a.id=#{id}
                </if>
                <if test="configid != null ">
                    and a.configid=#{configid}
                </if>
                <if test="depid != null ">
                    and a.depid=#{depid}
                </if>
                <if test="startcreatedate != null ">
                    and a.createdate >= #{startcreatedate }
                </if>
                <if test="endcreatedate != null ">
                    and <![CDATA[ a.createdate < #{endcreatedate }]]>
                </if>
                and a.deleted=false



            order by a.createdate desc

    </select>


</mapper>