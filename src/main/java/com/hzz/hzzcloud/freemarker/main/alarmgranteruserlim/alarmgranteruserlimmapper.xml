<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--报警转发规则的授权用户可见性配置表mapper -->
<mapper namespace="alarmgranteruserlimmapper">

    <!--查询报警转发规则的授权用户可见性配置表列表 -->
    <select id="selectlist" parameterType="java.util.HashMap"
            resultType="java.util.HashMap">
        select
                <!--     -->
                a.id,
                <!--  转发配置表的主键   -->
                a.configid,
                <!--  用户id   -->
                a.userid
        from
        subiaodb.alarmgranteruserlim as a

        where 1=1
                <if test="id != null ">
                    and a.id=#{id}
                </if>
                <if test="configid != null ">
                    and a.configid=#{configid}
                </if>
                <if test="userid != null ">
                    and a.userid=#{userid}
                </if>




    </select>


</mapper>